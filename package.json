{
  "name": "the-node-project",
  "version": "0.0.1",
  "description": "Events distribution manager",
  "main": "server.js",
  "scripts": {
    "start": "echo \"Error: no start implemented yet\" && exit 1",
    "dev": "nodemon --exec babel-node server.js",
    "devW": "SET NODE_ENV=dev&& nodemon --exec babel-node server.js",
    "test": "NODE_ENV=test mocha --compilers js:babel-core/register ./tests/*/*",
    "testW": "SET NODE_ENV=test&& mocha --compilers js:babel-core/register ./tests/*/*",
    "build": "NODE_ENV=production node -r babel-register node_modules/.bin/webpack --config=webpack.config.js"
  },
  "pre-commit": [
    "test"
  ],
  "author": "Jørgen Brække & Sean Scully",
  "license": "MIT",
  "dependencies": {
    "async": "^2.1.4",
    "babel-cli": "^6.18.0",
    "babel-loader": "^6.2.8",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.15.2",
    "connect-flash": "^0.1.1",
    "express": "^4.14.0",
    "express-session": "^1.14.2",
    "mongoose": "^4.7.3",
    "mongoose-validator": "^1.2.5",
    "nodemailer": "^2.7.0",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "react": "^15.4.1",
    "react-redux": "^5.0.1",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
    "redux-thunk": "^2.1.0",
    "webpack": "v2.1.0-beta.27"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.2"
  }
}
